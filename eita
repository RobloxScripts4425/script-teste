local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/LibraryV2/main/redzLib"))()

local Window = redzlib:CreateWindow({
    Title = "🥭Manga Hub🥭| Beta | Brookhaven RP🏡| V1.3 | 🇧🇷 Português 🇧🇷",
    Subtitle = "Anti Mandrake",
    SaveFolder = "RedzConfig"
})

-- Aba "Troll"
local TrollTab = Window:CreateTab("Troll")

TrollTab:CreateButton({
    Name = "Lagar Jogo",
    Callback = function()
        teleportGhostMeter()
    end
})

-- Aba "Item"
local ItemTab = Window:CreateTab("Item")

ItemTab:CreateButton({
    Name = "Dupe Livro",
    Callback = function()
        dupeBook()
    end
})

ItemTab:CreateButton({
    Name = "Dupe Extintor",
    Callback = function()
        dupeExtintor()
    end
})

ItemTab:CreateButton({
    Name = "Dupe Bola de Basquete",
    Callback = function()
        dupeBolaDeBasquete()
    end
})

ItemTab:CreateButton({
    Name = "Dupe Maca",
    Callback = function()
        dupeStretcher()
    end
})

redzlib:Init()
