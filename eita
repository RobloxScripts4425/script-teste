local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Crimsonx Scripthub",
   LoadingTitle = "CrimsonX|script vault",
   LoadingSubtitle = "by Stargazingüí´",
   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "crimsonx hub"
   },
   Discord = {
      Enabled = true,
      Invite = "https://discord.gg/kVmhWgvyQD", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Crimsonx key",
      Subtitle = "key in discord server",
      Note = "sussy kawaii üò≥üò≥",
      FileName = "crimsonx key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = false, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = true, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"https://pastebin.com/raw/Yrwyj1uE"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

local MainTab = Window:CreateTab("üè† Home", nil) -- Title, Image
local MainSection = MainTab:CreateSection("Main")

Rayfield:Notify({
   Title = "Correct key!",
   Content = "Thanks for using",
   Duration = 4.5,
   Image = nil,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "Okay!",
         Callback = function()
         print("The user tapped Okay!")
      end
   },
},
})

local Label = MainTab:CreateLabel("Thank you for using this scripthub ^^")

local Button = MainTab:CreateButton({
   Name = "Discord link",
   Callback = function()
   -- Create a function to copy the link and notify the user
local function copyLink()
    local link = "https://discord.gg/zBMgCaJt92"
    
    -- Assuming the executor has a clipboard API
    if setclipboard then
        setclipboard(link)  -- Copy the link to the clipboard
        
        -- Notify the user (You can change this to your preferred method of notification)
        print("Link copied to clipboard!")  -- This will display in the output
        game.StarterGui:SetCore("SendNotification", {
            Title = "Notification";
            Text = "Link has been added to your clipboard!";
            Duration = 5; -- Duration in seconds
        })
    else
        print("Clipboard function not available.")
    end
end

-- Call the function when the script runs
copyLink()
   end,
})

